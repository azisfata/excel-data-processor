# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Supabase Service Role Key (untuk server-side operations dan scripts)
# PENTING: Jangan expose key ini di frontend!
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Gemini AI Configuration
GEMINI_API_KEY=your_gemini_api_key_here

# JWT Secret for Authentication
# Generate a secure random string for production
JWT_SECRET=your-secret-key-change-this-in-production

# Admin User (for initial setup)
# Email yang akan digunakan saat menjalankan npm run create-admin
ADMIN_EMAILS=your-email@kemenkopmk.go.id
